mixin sGalery(data)
	// start sGalery
	+b.sGalery.section#sGalery&attributes(attributes)
		.container
			+e.row.row
				- for(var i=1; i < 9; i++)
					+sGaleryItem(i)
	// end sGalery

mixin sGaleryItem(imgNum)
	+e.col.col-6.col-sm-4.col-lg-3
		+e.img-link(href=`img/@2x/sWalls-${imgNum}.jpg` data-fancybox="sGaleryImages")
			img(loading="lazy" src=`img/@2x/sWalls-${imgNum}.jpg` alt="")